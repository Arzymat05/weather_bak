generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("db")
}

model ruWeather {
  id          Int     @unique @default(autoincrement())
  name        String  @unique
  temperature Int?
  day         String?
  humidity    Int?
  pressure    Int?
  wind        String?
}

model engWeather {
  id          Int     @unique @default(autoincrement())
  name        String  @unique
  temperature Int?
  day         String?
  humidity    Int?
  pressure    Int?
  wind        String?
}
